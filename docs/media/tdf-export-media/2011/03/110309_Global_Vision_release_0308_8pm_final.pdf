						var thisHtmlStr = $(this).html();
						var downloadLink = $(this).data("dnldlnk");
						var downloadLinkLow = $(this).data("dnldlnklow");
						var ckText = $(this).text();
						var n = ckText.indexOf("Download");
						var display_cart = $(this).data("display_cart");
