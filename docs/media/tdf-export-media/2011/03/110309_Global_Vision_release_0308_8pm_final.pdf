		<script type="text/javascript">
			$(function(){
				
				var supportsTouch = 'ontouchstart' in window || navigator.msMaxTouchPoints;
				if(supportsTouch){
					var aTouchEvent = "touchstart";
					$( ".dropdown-toggle" ).removeAttr( "data-hover");
					$( ".dropdown-toggle" ).attr( "data-toggle", "dropdown");
				}else{
					var aTouchEvent = "click";
					
					$( ".dropdown-toggle" ).removeAttr( "data-toggle");
					$( ".dropdown-toggle" ).attr( "data-hover", "dropdown");
				}

				function ajaxDDMenuContent(){
					var viewportWidth = $(window).width();
					$( "li.hoverSectionArticles" ).unbind();

					
					if(viewportWidth > 768){
						
						$( "li.hoverSectionArticles" ).on( "mouseover", function() {
							var sec_id = $(this).data("sectionid");
							var appendClass = "articlesAfterHere_" + $(this).data("classid");
							var sectionType= $(this).data("sectioncontenttype");

							
							var request = $.ajax({
								url: "/rmt/rmtMng.cfc?method=getHeaderContent",
								type: "POST",
								data: {railRootId : sec_id,
									   sectionType: sectionType
									  },
								dataType: "html"
							});

							request.done(function(data) {
								$('.removeItems').remove();
								$( "." + appendClass).after(data);
							});
						});
						$(".vertNav").after('<li class="displayInline removeItems col-md-3 hidden-xs hidden-sm"></li><li class="displayInline removeItems col-md-3 hidden-xs hidden-sm"></li><li class="displayInline removeItems col-md-3 hidden-xs hidden-sm"></li>');
					}else{
						
						$( "li.hoverSectionArticles" ).unbind();
					};
				};

				function menuFlyout(){
					var viewportWidth = $(window).width();
					$( "a.topLevelLink" ).unbind();
					$( ".menuClose" ).unbind();

					
					if(viewportWidth <= 768 || viewportWidth >= 768 && viewportWidth <= 1024 && supportsTouch && window.matchMedia("(orientation: landscape)").matches){
						
						$('a.topLevelLink').on( aTouchEvent, function(e) {
							$(this).attr('href','javascript:void(0)');
						});

						
						$( ".topLevelLink" ).on( aTouchEvent, function(e) {
							e.preventDefault();

							var showBrand = $(this).data("brandid");

							$('.brand_nav_container').css('display','none');

							
							 // run the effect
							$( "#slide_right").animate({
								left: "325"
							}, {queue: false,duration: 1000}, function() {
								// Animation complete.
								$("#left_flyout_menu").css('z-index','9999');
							});
							$( "#left_flyout_menu").animate({
								left: "0"
							}, {queue: false,duration: 1000}, function() {
								// Animation complete.
							});
							$("#brand_" + showBrand).css('display','block');

							$( ".dropdown-toggle" ).removeAttr( "data-hover");
							$( ".dropdown-toggle" ).attr( "data-toggle", "dropdown");
						});

						$( ".menuClose" ).on( aTouchEvent, function() {
							$("#left_flyout_menu").css('z-index','9');
							 // run the effect
							
							$( "#slide_right").animate({
								left: "0"
							}, {queue: false,duration: 1000}, function() {
								// Animation complete.
								$("#left_flyout_menu").css('z-index','9999');
							});
							$( "#left_flyout_menu").animate({
								left: "-325"
							}, {queue: false,duration: 1000}, function() {
								// Animation complete.
							});

						});

					}else{
						$('a.topLevelLink').each(function( index ) {
							
							var thisHref = $(this).data("linkref");
							$(this).attr('href',thisHref);
						});
					}
				};

				function testViewport(){
					var viewportWidth = $(window).width();

					
					if(viewportWidth <= 768 || viewportWidth >= 768 && viewportWidth <= 1024 && supportsTouch && window.matchMedia("(orientation: landscape)").matches){
					//if(viewportWidth <= 768 || (viewportWidth >= 768 && viewportWidth <= 1024 && window.matchMedia("(orientation: landscape)").matches)){
						
						
						$( ".headernavigation").addClass('displayNone');
						$( ".brand").removeClass('dropdown');

						$( ".dropdown-toggle" ).removeAttr( "data-hover");
						$( ".dropdown-toggle" ).attr( "data-toggle", "dropdown");

						
						$('a.topLevelLink').on( aTouchEvent, function(e) {
							$(this).attr('href','javascript:void(0)');
						});


					}else{
						$( ".brand").addClass('dropdown');
						$( ".dropdown-menu").removeClass('displayNone');
						$( ".dropdown-toggle" ).removeAttr( "data-toggle");
						$( ".dropdown-toggle" ).attr( "data-hover","dropdown");

						$('a.topLevelLink').each(function( index ) {
							
							var thisHref = $(this).data("linkref");
							$(this).attr('href',thisHref);
						});
					}
				}

				ajaxDDMenuContent();
				menuFlyout();
				testViewport();

				
				$( window ).resize(function() {
					ajaxDDMenuContent();
					menuFlyout();
					testViewport();
				});
			});
		</script>

	
