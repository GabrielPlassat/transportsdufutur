<script type="text/javascript">
	var tabchangeinprogress = false;
	function ChangeDailyTab(tab) {
		tabchangeinprogress = true;
		var oldActive = $('div.dailyTeaser ul li.selectedTab');
		var oldTabId = $('div.dailyTeaser ul li.selectedTab').data('tab');
		$('div#dailyTeaserContent div[data-tab=' + oldTabId + ']').fadeOut('slow', function () {
			oldActive.removeClass('selectedTab').data('tab');
			tab.addClass('selectedTab');
			var tabNumber = tab.data('tab');
			$('div#dailyTeaserContent div[data-tab=' + tabNumber + ']').fadeIn('slow', function () { tabchangeinprogress = false; });
			var detailnode = $("div#dailyTeaserContent div[data-tab=" + tabNumber + "] .ellipsis-daily");
			
		});
	}

	$(document).ready(function () {

		$('div.dailyTeaser ul.tab li').on('click', function () {
			if (tabchangeinprogress) {
				return;
			}
			ChangeDailyTab($(this));
		});

		$('.dailyTeaser .video-link').click(function (event) {
			event.preventDefault();
			$.pgwModal({
				target: 'div#dailyTeaserContent div[data-tab="' + $('.dailyTeaser .video').attr('data-tab') + '"] .video-element',
				closeCssClass: 'video-close',
				maxWidth: 960,
				maxHeight: 540
			});

			$(document).click(function (event) {
				if (!$(event.target).closest('.pm-body').length) {
					$.pgwModal('close');
				}
			});

			var rand = Math.floor(Math.random() * 1000) + 1;
			var newId = "pm-container-videoContainer" + rand;
			$(".pm-container #videoContainer").attr("id", newId);
			jwplayer(newId).setup({
				flashplayer: "/Content/videoplayer/player.swf"
			});
			return false;
		});

		    ChangeDailyTab($('li[data-tab=1]'));

	});
</script>

